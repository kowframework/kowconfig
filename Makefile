# Makefile for the AW_Config
#
# @author Marcelo Cora√ßa de Freitas <marcelo.batera@gmail.com> 


libs:
	gnatmake -P awconfig.gpr
tests:
	gnatmake -P awconfig-tests.gpr

all: libs



clean-libs:
	gnatclean -P awconfig.gpr
clean-tests:
	gnatclean -P awconfig-tests.gpr
clean: clean-tests clean-libs
	@echo "All clean"



docs:
	@-./gendoc.sh
	@echo "The documentation is generated by a bash script. Then it might fail in other platforms"
