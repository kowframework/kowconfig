<?xml version="1.0" encoding="utf-8" standalone="no"?>

<!--
There is no DOCTYPE here because Aw_Config is validating the document by itself.
It's a simple document and there is no need to a DTD or a Schema definition.

One is planned in the future, but not now (other priorities)!

// -->


<config>
	<section name="Main Section">
		<key name="company name" value="My Test Company"/>
		<key name="company size" value="medium sized"/>
		<section name="Contact Information">
			<key name="address" value="Piraporinha St, number 123"/>
		</section>
	</section>
	<section name="Database">
		<key name="host" value="localhost"/>
		<key name="user" value="root"/>
		<key name="password" value="myrootpassword"/>
		<key name="database" value="My_Database"/>
		<key name="engine">MySQL</key>
	</section>
</config>
